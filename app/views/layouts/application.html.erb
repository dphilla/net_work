<!DOCTYPE html>
<html>
  <head>
    <title>NetWork</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if current_user %>
    <%= current_user.username %>
    <%= link_to "Logout", logout_path, method: :delete %>
  <% else %>
  <% end %>
    <%= yield %>
  </body>
</html>
